Algoritmo "CONSUMO"

Var
    distancia : inteiro
    consumoMedio,combustivel : real


Inicio
        escreva("Qual foi a distancia percorrida: ")
        leia(distancia)
        escreva("Quanto de combustivel gastou: ")
        leia(combustivel)
        
        consumoMedio <- distancia/combustivel
        
        escreva("O consumo médio foi de: ",consumoMedio:8:3)




Fimalgoritmo
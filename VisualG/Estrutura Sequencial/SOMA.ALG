Algoritmo "SOMA"

Var
     y,x,soma : inteiro



Inicio
        escreval("Digite dois numeros inteiros")
        escreva("Primeiro numero: ")
        leia(x)
        escreva("Segundo numero: ")
        leia(y)
        soma <- y+x
        escreva("A soma dos dois numeros é igual a ",soma)
Fimalgoritmo
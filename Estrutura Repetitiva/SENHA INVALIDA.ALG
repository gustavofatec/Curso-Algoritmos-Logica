Algoritmo "SENHA INVALIDA"

Var
     senha : inteiro


Inicio

       escreva("Digite a senha: ")
       leia(senha)
       
       enquanto senha <> 2002 faca
         escreval("Senha invalida !!")
         escreval
         escreva("Digite novamente a senha: ")
         leia(senha)
       fimenquanto
       
       escreval("Acesso permitido !!")


Fimalgoritmo